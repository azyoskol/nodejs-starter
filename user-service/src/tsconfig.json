{
    "compilerOptions": {
        "module": "commonjs",
        "esModuleInterop": true,
        "target": "es6",
        "noImplicitAny": true,
        "moduleResolution": "node",
        "sourceMap": true,
        "outDir": "../dist/",
        "baseUrl": ".",
        "skipLibCheck": true,
        "paths": {
            "repository/*":["../../repository/*"],
            "repository":["../../repository"],
            "models":["../../models"],
            "models/*":["../../models/*"]
        },
    },
    "exclude": [
      "node_modules",
      "node_modules/@types/**/*",
      "**/*.d.ts"
    ]
  }